# Project-scoped PATH interceptors (no console output)
unset VIRTUAL_ENV
export PROJECT_BIN="$(pwd)/tools/interceptors/bin"
export PATH="$PROJECT_BIN:$PATH"

# Remove global aliases only inside this directory
unalias python 2>/dev/null || true
unalias python3 2>/dev/null || true

# Refresh command lookup cache
rehash 2>/dev/null || hash -r || true
export PATH="$PROJECT_BIN:${PATH//:$PROJECT_BIN/}"

